{
  "name": "wundf-web",
  "version": "1.0.0",
  "description": "static website realized with way to complicated tools",
  "main": "app.js",
  "scripts": {
    "serve": "nodemon --exec 'babel-node server.js' --ignore assets/",
    "build:js": "browserify client.js -t babelify --extension=.jsx --debug | exorcist assets/js/bundle.js.map > assets/js/bundle.js",
    "watch:js": "nodemon --exec 'npm run build:js' -e js,jsx --ignore assets/ -q",
    "build:css": "echo no_css_yet",
    "watch:css": "nodemon --exec 'npm run build:css' -e styl --ignore assets/ -q",
    "build": "npm run build:js && npm run build:css",
    "build:watch": "parallelshell 'npm run watch:js -s' 'npm run watch:css -s' 'npm run serve -s'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wunderundfitzig/website.git"
  },
  "author": "Manuel Reich",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wunderundfitzig/website/issues"
  },
  "homepage": "https://github.com/wunderundfitzig/website#readme",
  "dependencies": {
    "express": "^4.13.4",
    "node-fetch": "^1.5.2",
    "path": "^0.12.7",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "serve-favicon": "^2.3.0",
    "superagent": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "exorcist": "^0.4.0",
    "nodemon": "^1.9.2",
    "parallelshell": "^2.0.0"
  }
}
